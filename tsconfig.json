{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "module": "esnext",
    "types": [
      "vite/client"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    },
    "jsx": "react"
  }
}
